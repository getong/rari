{
  "name": "@rari/docs",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Runtime Accelerated Rendering Infrastructure (Rari) Documentation",
  "author": "Ryan Skinner",
  "license": "MIT",
  "scripts": {
    "build": "pnpm clean && pnpm typecheck && vite build",
    "dev": "NODE_OPTIONS='--max-http-header-size=1048576' vite",
    "start": "node node_modules/rari/dist/cli.js start",
    "clean": "rm -rf .rari && rm -rf dist",
    "lint": "oxlint && eslint",
    "lint:fix": "oxlint --fix && eslint --fix",
    "typecheck": "tsgo"
  },
  "dependencies": {
    "markdown-it": "^14.1.0",
    "rari": "^0.2.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shiki": "^3.13.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.1",
    "@types/react-dom": "^19.2.0",
    "@typescript/native-preview": "7.0.0-dev.20250923.1",
    "eslint": "^9.37.0",
    "oxlint": "^1.20.0",
    "tailwindcss": "^4.1.14",
    "vite": "^7.1.9"
  }
}
